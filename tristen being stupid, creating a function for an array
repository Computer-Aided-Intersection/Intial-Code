#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <math.h>

int main(){
	float v = (float)(vda[i][0]);
	float d = (float)(vda[i][1]);
	float a = (float)(vda[i][2]);
	time[i] = time(v,d,a);
	
float time (float v, float d, float a){
	float time;
	for(i = 0; i < n; i++){
		float x = (-v/a);
		float y = sqrt(v*v-2*a*d*-1)/a; //Calculating time to intersection
		time = x + y; 
	}
	return time;
}
